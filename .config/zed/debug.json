[
  {
    "adapter": "JavaScript",
    "label": "Node.js: Current File",
    "type": "node",
    "request": "launch",
    "program": "$ZED_FILE",
    "runtimeExecutable": "node",
    "runtimeArgs": [
      "--expose-gc"
    ],
    "console": "integratedTerminal",
    "skipFiles": [
      "<node_internals>/**"
    ]
  },
  {
    "adapter": "JavaScript",
    "label": "Node.js: Current File (watch mode)",
    "type": "node",
    "request": "launch",
    "program": "$ZED_FILE",
    "runtimeExecutable": "node",
    "runtimeArgs": [
      "--watch"
    ],
    "console": "integratedTerminal",
    "skipFiles": [
      "<node_internals>/**"
    ]
  },
  {
    "adapter": "JavaScript",
    "label": "TypeScript: Current File",
    "type": "node",
    "request": "launch",
    "program": "$ZED_FILE",
    "runtimeExecutable": "npx",
    "runtimeArgs": [
      "tsx",
      "$ZED_FILE"
    ],
    "console": "integratedTerminal",
    "skipFiles": [
      "<node_internals>/**"
    ]
  },
  {
    "adapter": "JavaScript",
    "label": "TypeScript: Current File (watch mode)",
    "type": "node",
    "request": "launch",
    "program": "$ZED_FILE",
    "runtimeExecutable": "npx",
    "runtimeArgs": [
      "tsx",
      "--watch",
      "$ZED_FILE"
    ],
    "console": "integratedTerminal",
    "skipFiles": [
      "<node_internals>/**"
    ]
  },
  {
    "adapter": "JavaScript",
    "label": "Chrome: Launch",
    "type": "chrome",
    "request": "launch",
    "url": "http://localhost:1234",
    "webRoot": "$ZED_WORKTREE_ROOT",
    "skipFiles": [
      "<node_internals>/**"
    ]
  }
]
