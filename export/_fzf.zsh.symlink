# Setup fzf
# ---------
if [[ ! "$PATH" =~ "$HOME/.dotfiles/export/_fzf.symlink/bin" ]]; then
  export PATH="$PATH:$HOME/.dotfiles/export/_fzf.symlink/bin"
fi

# Man path
# --------
if [[ ! "$MANPATH" =~ "$HOME/.dotfiles/export/_fzf.symlink/man" && -d "$HOME/.dotfiles/export/_fzf.symlink/man" ]]; then
  export MANPATH="$MANPATH:$HOME/.dotfiles/export/_fzf.symlink/man"
fi

# Key bindings
# ------------
source "$HOME/.dotfiles/export/_fzf.symlink/shell/key-bindings.zsh"
